        <name>NeedIt Populate Email Field</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '' || newValue == oldValue) {
      return;
   }
	var a = new GlideAjax('GetEmailAddress');
	a.addParam('sysparm_name','getEmail');
	a.addParam('sysparm_userID',g_form.getValue('u_requested_for'));
	
	a.getXML(cbk);
	
	function cbk(response){
		var b = response.responseXML.documentElement.getAttribute("answer");
		g_form.clearValue('u_req_email');
		g_form.setValue('u_req_email',b);
	}

}]]></script>
